{
  "name": "ionic-native-notificare-push",
  "description": "Official Notificare Ionic Native Plugin.",
  "version": "2.4.0-beta.1",
  "license": "Apache",
  "author": "Notificare",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run build:esm && npm run build:ngx && npm run build:es5",
    "build:esm": "ts-node -P scripts/tsconfig.json scripts/tasks/build-esm",
    "build:es5": "ts-node -P scripts/tsconfig.json scripts/tasks/build-es5",
    "build:ngx": "ts-node -P scripts/tsconfig.json scripts/tasks/build-ngx",
    "release": "npm run build && ts-node -P scripts/tsconfig.json scripts/tasks/publish.ts",
    "lint": "gulp lint",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\""
  },
  "dependencies": {
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@ionic-native/core": "^5.28.0",
    "@types/cordova": "0.0.34",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.159",
    "@types/node": "^12.12.54",
    "@types/rimraf": "^3.0.0",
    "@types/webpack": "^4.41.21",
    "ajv": "^6.12.3",
    "async-promise-queue": "^1.0.5",
    "conventional-changelog-cli": "^2.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "dgeni": "^0.4.12",
    "dgeni-packages": "0.16.10",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-tslint": "^8.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "natives": "^1.1.6",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-ionic-rules": "0.0.21",
    "typedoc": "^0.18.0",
    "typescript": "~3.8.3",
    "typescript-tslint-plugin": "0.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "unminified-webpack-plugin": "^2.0.0",
    "webpack": "^4.44.1",
    "winston": "^3.3.3",
    "zone.js": "^0.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ]
  }
}
